<?xml version="1.0" encoding="utf-8"?>
<Module>
<ModulePrefs title="谷歌地图" description="google地图查看，方便你查看全国各地地图" author="迈弄" author_affiliation="迈弄" author_link="http://www.mynon.cn" height="330" scaling="false">
	<Require feature="dynamic-height"/>
</ModulePrefs>
<Content type="html">
<![CDATA[
<style type="text/css">
body,div{font-size:12px;}
.wd{padding:2px 0;font-size:12px;}
</style>
<script type="text/javascript">
function showWData(data){
	var data=eval('('+data+')');
	var div = _gel("mn_85");
	var htmlCode='';
	if(data.u && data.t){
		var htmlCode = '<img style="margin-bottom:-3px;width:16px;height:16px;" src="'+unescape(data.i)+'" /> <a href="http://www.mynon.cn/files/bk_click.php?id=m_'+data.w+'&uid='+unescape(data.u)+'&f=os_ty" target="_blank">'+unescape(data.t)+'</a>';
		div.innerHTML=htmlCode;
	}else{
		div.style.display="none";
	}
	_IG_AdjustIFrameHeight();
}
function getWData(wid) {
	var url='http://www.mynon.cn/widget/data/interlink/tianya/'+wid+'/json';
	_IG_FetchContent(url, showWData, {refreshInterval:0});
}
function initWData() {
	var wid=85;
	getWData(wid);
}
_IG_RegisterOnloadHandler(initWData);
</script>
<div style="text-align:center"><div><a target="_blank" href="http://www.mynon.cn/widget/data/game/85/page">使用全屏模式</a></div><iframe width="100%" height="308" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="http://ditu.google.cn/maps?hl=zh-CN&amp;ie=UTF8&amp;om=1&amp;s=AARTsJpK3U1mojZEiPfm9u5GkK3oaUV3lQ&amp;ll=38.891033,106.259766&amp;spn=32.668313,56.25&amp;z=4&amp;output=embed"></iframe></div>
<div id="mn_85" class="wd"></div>
]]>
</Content>
</Module>