<?xml version="1.0" encoding="utf-8"?>
<Module>
<ModulePrefs title="数独" description="风靡全球的数独游戏，在每一个九宫格不重复的填上1至9的数字，鼠标和数字键操作" author="迈弄" author_affiliation="迈弄" author_link="http://www.mynon.cn" height="325" scaling="false">
	<Require feature="dynamic-height"/>
</ModulePrefs>
<Content type="html">
<![CDATA[
<style type="text/css">
.wd{padding:2px 0;font-size:12px;}
</style>
<script type="text/javascript">
function showWData(data){
	var data=eval('('+data+')');
	var div = document.getElementById("mn_37");
	var htmlCode='';
	if(data.u && data.t){
		var htmlCode = '<img style="margin-bottom:-3px;width:16px;height:16px;" src="'+unescape(data.i)+'" /> <a href="http://www.mynon.cn/files/bk_click.php?id=m_'+data.w+'&uid='+unescape(data.u)+'&f=os_ty" target="_blank">'+unescape(data.t)+'</a>';
		div.innerHTML=htmlCode;
	}else{
		div.style.display="none";
	}
	_IG_AdjustIFrameHeight();
}
function getWData(wid) {
	var url='http://www.mynon.cn/widget/data/interlink/tianya/'+wid+'/json';
	_IG_FetchContent(url, showWData, {refreshInterval:0});
}
function initWData() {
	var wid=37;
	getWData(wid);
}
_IG_RegisterOnloadHandler(initWData);
</script>
<div style="text-align:center"><iframe scrolling="no" width="280" height="304" marginwidth="0" marginheight="0" frameborder="0" src="../lt/list_sudoku.htm"></iframe></div>
<div id="mn_37" class="wd"></div>
]]>
</Content>
</Module>